
<%= link_to 'new', new_menu_path %>
<% @menus.each do |menu| %>
    
     <table>
        <tr>
            <td>  
                <%= link_to menu.name, menu_path(menu) %>
            </td>
            <td>
               <%= menu.total_distance %>
            </td>
        </tr>
    </table>

    
<% end %>
<%= area_chart @menus.pluck(:created_at,:total_distance), max: 12000 %>
